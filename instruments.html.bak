    <style>
	

      
	*{
		box-sizing: border-box;
		text-align: center;	
	}
	
		
	html, body {
		font-family: 'Montserrat', sans-serif;
		margin: 0;
		height: 100%;
		background: #000000;
		color: white;
	}
	/* Header/logo Title */
	.header {
		padding: 20px;
		text-align: center;
		background: #000000;
		color: white;
		margin:0;
		font-family: 'typoliner-rwlight', Arial, sans-serif;
	}
	
	/* Style the top navigation bar */
	.topnav {
		overflow: hidden; /* Hide overflow */
		background-color: #7f7f7f; /* Dark background color */
		border:0px solid #ccc;
		border-width:0px 0;
		list-style:none;
		margin:0;
		padding:0;
		text-align:center;
  }
	
	/* Style the navigation bar links */
	.topnav a {
		display: block;
		justify-content: center;		
		color: white; /* White text color */
		text-align: center; /* Center the text */
		padding: 14px 20px; /* Add some padding */
		text-decoration: none; /* Remove underline */
	}

	/* Change color on hover/mouse-over */
	.topnav a:hover {
		background-color: #ddd; /* Grey background color */
		color: black; /* Black text color */
		text-decoration: none; /* Remove underline */
	}

	.topnav li{
		display:inline;
	}
	.topnav a{
		display:inline-block;
		padding:10px 30px;
	}
  
      .topnav a.active{text-decoration:underline;}
      
	a{
		color:#c00;
		text-decoration:none;
		font-weight:bold;
	}
      
  a:hover{
  		text-decoration:underline;
  }
	
      .footer {
        font-family: 'Montserrat', sans-serif; 
        font-size:13px;
        padding: 20px; /* Some padding */
        overflow: hidden
        text-align: left;
        background: #7f7f7f; /* Grey background */
        position: relative;
        bottom:0;
        margin-top: -100px; /* negative value of footer height */
        height: 150px;
        clear:both;
        padding-top:20px;
        color:white;
        
        -webkit-column-count: 2; /* Chrome, Safari, Opera */
        -moz-column-count: 2; /* Firefox */
        column-count: 2;
        -webkit-column-width: 100px; /* Chrome, Safari, Opera */
        -moz-column-width: 100px; /* Firefox */
        column-width: 100px;

      }
      
      .footer a{        
        font: 'Montserrat', sans-serif;
        color:white;
      }
      
      .footer a:hover{
        text-decoration:underline;
      }

      .fa {
        padding: 0px;
        font-size: 14px;
        width: 30px;
        text-align: center;
        text-decoration: none;
        border-radius: 50%;
       }

      .fa:hover {
          opacity: 0.7;
      }     

	
	#wrap {
		min-height: 100%;
	}

	#main {
		overflow:auto;
		padding-bottom:100px; /* this needs to be bigger than footer height*/
	}
	
	#floated {
		float: left;
	}
	
  .popup {
      display: none;
      position: fixed;
      padding: 10px;
      width: 80%;
      left: 10%;
      margin-left: 0px;
      height: auto;
      top: 20%;
      margin-top: 0px;
      background: #FFF;
      z-index: 20;
    	color: black;
      text-align:left;
    }

    #popup1 {
      -webkit-box-shadow:  0px 0px 0px 9999px rgba(0, 0, 0, 0.5);
      box-shadow:  0px 0px 0px 9999px rgba(0, 0, 0, 0.5);
    }
    
      
	@font-face {
		font-family: 'typoliner-rwlight';
		src: url('typoliner-rw-light-webfont.woff2') format('woff2'),
			 url('typoliner-rw-light-webfont.woff') format('woff');
		font-weight: normal;
		font-style: normal;
	}
      
  @media screen and (max-width: 600px) {
    .topnav a {display: none;}
    .topnav a.icon {
      float: right;
      display: block;
    }
  }

  @media screen and (min-width: 601px) {
    .topnav a.icon {display: none;}
  }      
      
  @media screen and (max-width: 600px) {
    .topnav.responsive {position: relative;}
    .topnav.responsive .icon {
      position: absolute;
      right: 0;
      top: 0;
    }
    .topnav.responsive a {
      float: none;
      display: block;
      text-align: left;
    }
  }



